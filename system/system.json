{
	"id": "fallout",
	"title": "Fallout 2d20",
	"description": "The Unofficial Fallout 2d20 System for FoundryVTT",
	"version": "11.3.6",
	"compatibility": {
		"minimum": "11",
		"verified": "11"
	},
	"authors": [
		{
			"name": "Paul Maskelyne",
			"discord": "muttley",
			"email": "mu77ley@gmail.com",
			"mastodon": "@muttley@dice.camp"
		},
		{
			"name": "HappySteve"
		}
	],
	"esmodules": [
		"fallout-compiled.mjs"
	],
	"styles": [
		"css/fallout.css"
	],
	"packs": [
		{
			"name": "addictions",
			"label": "Addictions",
			"system": "fallout",
			"path": "packs/addictions",
			"type": "Item",
			"private": false,
			"flags": {}
		},
		{
			"name": "ammunition",
			"label": "Ammunition",
			"system": "fallout",
			"path": "packs/ammunition",
			"type": "Item",
			"private": false,
			"flags": {}
		},
		{
			"name": "apparel",
			"label": "Apparel",
			"system": "fallout",
			"path": "packs/apparel",
			"type": "Item",
			"private": false,
			"flags": {}
		},
		{
			"name": "apparel_mods",
			"label": "Armor & Clothing Mods",
			"system": "fallout",
			"path": "packs/apparel_mods",
			"type": "Item",
			"private": false,
			"flags": {}
		},
		{
			"name": "consumables",
			"label": "Consumables",
			"system": "fallout",
			"path": "packs/consumables",
			"type": "Item",
			"private": false,
			"flags": {}
		},
		{
			"name": "system-documentation",
			"label": "System Documentation",
			"system": "fallout",
			"path": "packs/system-documentation",
			"type": "JournalEntry",
			"private": false,
			"flags": {}
		},
		{
			"name": "fallout-macros",
			"label": "Fallout Macros",
			"system": "fallout",
			"path": "packs/fallout-macros",
			"type": "Macro",
			"private": false,
			"flags": {}
		},
		{
			"name": "skills",
			"label": "Skills",
			"system": "fallout",
			"path": "packs/skills",
			"type": "Item",
			"private": false,
			"flags": {}
		}
	],
	"packFolders": [
		{
			"name": "Fallout RPG",
			"sorting": "a",
			"color": "#293f5f",
			"packs": [
				"fallout-macros",
				"system-documentation"
			],
			"folders": [
				{
					"name": "Items & Abilities",
					"sorting": "a",
					"color": "#141c2b",
					"packs": [
						"addictions",
						"ammunition",
						"apparel_mods",
						"apparel",
						"consumables",
						"skills"
					]
				}
			]
		}
	],
	"languages": [
		{
			"lang": "de",
			"name": "Deutsch",
			"path": "i18n/de.json",
			"flags": {}
		},
		{
			"lang": "en",
			"name": "English",
			"path": "i18n/en.json",
			"flags": {}
		},
		{
			"lang": "fr",
			"name": "Français",
			"path": "i18n/fr.json",
			"flags": {}
		},
		{
			"lang": "pl",
			"name": "Polski",
			"path": "i18n/pl.json",
			"flags": {}
		},
		{
			"lang": "pt-BR",
			"name": "Português (Brasil)",
			"path": "i18n/pt_BR.json",
			"flags": {}
		}
	],
	"socket": true,
	"url": "https://github.com/Muttley/foundryvtt-fallout",
	"manifest": "https://raw.githubusercontent.com/Muttley/foundryvtt-fallout/main/system/system.json",
	"download": "https://github.com/Muttley/foundryvtt-fallout/releases/latest/download/fallout.zip",
	"background": "systems/fallout/assets/images/please-stand-by.webp"
}
